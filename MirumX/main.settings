[Themes and colours]
	[Loading screen]
		loading.color=f8
		loading.title=Loading
		loading.width=21
		loading.height=3
	[Window theme]
		window.title.suffix=- MirumX
		window.width=100
		window.height=30
		window.color=3f
		[ Window positioner API]
			window.pos=enable
			window.pos.x=50
			window.pos.y=70
	[Exit screen]
		exit.window.title=Exit
		exit.window.width=45
		exit.window.height=14
		exit.window.color=f0
		exit.window.color.ASCII=fd
		exit.window.color.copyrighttext=f8
		exit.window.color.linktext=f9
	[ Input & Misc ]
		input.password.mask=enable
		GUI.inputbox=disable
		GUI.inputbox.password.mask=enable
		GUI.sounds=enable
