[Advanced configuration]
	[ # Enable/Disable APIs #]
		[ # Enables the Boot Timer API # ]
			API.Timer=enable
		[ # Enables the Cleanup API on boot # ]
			API.Cleanup=enable
		[ # Fixes the List API bug when options 9+ used - Impacts speed by up to 20% # ]
			API.List.Wrapping=enable
		[ # Enables developer mode (@echo ON instead of @echo OFF) #]
			API.DeveloperMode=disable
		[ # Enables the Crash helper API, allows user to see error codes at Crash event #]
			API.CrashHelper=enable
		[ # Override how APIs are launched # ]
			API=call API.bat 
	[ # Specifies variables used by MirumX internally, usually no need edit (Can be useful when modding/theming MirumX) # ]
		echo=<nul set /p =
		goto=goto :goto
		back=goto :back
		terminate=goto :exit
		big=mode con: cols=9999 lines=9999